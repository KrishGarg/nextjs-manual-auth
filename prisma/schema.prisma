generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String           @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  password String
  tokens   LoggedInClient[]
}

type LoggedInClient {
  tokenId String
  client  String
  ip      String
}
